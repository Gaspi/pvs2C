
demo : THEORY
BEGIN
  

% Arithmetic

  Arr : TYPE = [below(10) -> below(10) ]

  a:below(10) = 8
  
  b:Arr = lambda (x:below(10)) : x

  incr(t:Arr) : Arr = t with [ (t(0)) := t(9) ]

%  foo(t:Arr) : Arr = t with [ (0) :=  0 + 1]
   foo(t:Arr) : Arr = t with [ (0) := t(9) + 1, (t(0)) :=  t(0) + 1 ]

END demo
